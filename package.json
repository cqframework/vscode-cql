{
  "name": "cql",
  "version": "0.7.11-SNAPSHOT",
  "displayName": "Clinical Quality Language (CQL)",
  "description": "Syntax highlighting, linting, and execution for the HL7 Clinical Quality Language (CQL) for VS Code",
  "publisher": "cqframework",
  "license": "Apache-2.0",
  "author": "Alphora",
  "bugs": "https://github.com/cqframework/vscode-cql/issues",
  "keywords": [
    "cqframework",
    "clinical quality language",
    "cql",
    "fhir",
    "hl7"
  ],
  "icon": "icons/cqframework.png",
  "preview": true,
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    },
    "virtualWorkspaces": false
  },
  "engines": {
    "vscode": ">=1.75.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cqframework/vscode-cql"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters",
    "Snippets"
  ],
  "main": "./dist/extension",
  "activationEvents": [
    "workspaceContains:**/*.cql"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cql",
        "extensions": [
          ".cql"
        ],
        "configuration": "./language-configuration.json",
        "aliases": [
          "CQL",
          "Cql"
        ]
      }
    ],
    "snippets": [
      {
        "language": "cql",
        "path": "./snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "cql",
        "scopeName": "source.cql",
        "path": "./syntaxes/cql.tmLanguage.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "CQL"
    },
    "commands": [
      {
        "command": "cql.open.serverLog",
        "title": "Open Server Logs",
        "category": "CQL"
      },
      {
        "command": "cql.open.clientLog",
        "title": "Open Client Logs",
        "category": "CQL"
      },
      {
        "command": "cql.open.logs",
        "title": "Open Logs",
        "category": "CQL"
      },
      {
        "command": "cql.action.viewElm.xml",
        "title": "View ELM - XML",
        "category": "CQL"
      },
      {
        "command": "cql.action.viewElm.json",
        "title": "View ELM - JSON",
        "category": "CQL"
      },
      {
        "command": "cql.action.executeCql",
        "title": "Execute CQL",
        "category": "CQL"
      },
      {
        "command": "cql.action.executeCql.selectLibraries",
        "title": "Execute CQL - Select Libraries",
        "category": "CQL"
      },
      {
        "command": "cql.action.executeCql.selectTestCases",
        "title": "Execute CQL - Select Test Cases",
        "category": "CQL"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "cql.action.executeCql",
          "when": "editorLangId == cql",
          "group": "1_cqlactions@1.execute@1"
        },
        {
          "command": "cql.action.executeCql.selectTestCases",
          "when": "editorLangId == cql",
          "group": "1_cqlactions@1.execute@3"
        },
        {
          "command": "cql.action.viewElm.xml",
          "when": "editorLangId == cql",
          "group": "1_cqlactions@2.elm@1"
        },
        {
          "command": "cql.action.viewElm.json",
          "when": "editorLangId == cql",
          "group": "1_cqlactions@2.elm@2"
        }
      ],
      "commandPalette": [
        {
          "command": "cql.action.executeCql",
          "when": "false"
        },
        {
          "command": "cql.action.executeCql.selectTestCases",
          "when": "false"
        },
        {
          "command": "cql.action.viewElm.xml",
          "when": "false"
        },
        {
          "command": "cql.action.viewElm.json",
          "when": "false"
        }
      ]
    }
  },
  "resolutions": {
    "color-name": "1.1.3"
  },
  "overrides": {
    "form-data": "^4.0.5"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "vscode-test"
  },
  "javaDependencies": {
    "cql-language-server": {
      "groupId": "org.opencds.cqf.cql.ls",
      "artifactId": "cql-ls-service",
      "version": "4.2.0-SNAPSHOT"
    }
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/expand-tilde": "^2.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.183",
    "@types/mocha": "^10.0.10",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^16.18.34",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.73.0",
    "@types/winreg": "^1.2.31",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.3.6",
    "chai": "4.5",
    "mocha": "^11.7.5",
    "mock-fs": "^5.5.0",
    "prettier": "^3.3.3",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "expand-tilde": "^2.0.2",
    "find-java-home": "1.2.2",
    "fs-extra": "^8.1.0",
    "glob": "^10.5.0",
    "lodash": "^4.17.23",
    "node-fetch": "2.6.7",
    "pino": "^10.3.1",
    "tslib": "^2.4.0",
    "vscode-languageclient": "^7.0.0",
    "vscode-uri": "^3.0.8",
    "winreg": "^1.2.4",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-transport-vscode": "^0.1.0"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2,
    "arrowParens": "avoid"
  }
}